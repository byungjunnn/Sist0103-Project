		stuName=name; //this.은 생략가능
		stuGen=gender;
		stuAddr=addr;
	}
	
	//출력 메서드
	public void writeMemberData() {
		cnt++;
		System.out.println("**학생 "+cnt+" 정보**");
		System.out.println("이름: "+stuName);
		System.out.println("성별: "+stuGenu);
		System.out.println("주소: "+stuAddr);
	}
}
