package day0112;

class Shop{
	
	private String num;
	private String sName;
	private String sPrice;
	private String sColor;
	
	public Shop(String num, String sName, String sPrice, String sColor) {
		this.num=num;
		this.sName=sName;
		this.sPrice=sPrice;
		this.sColor=sColor;
	}
	
	public static void getsTitle() {
		System.out.println(["쌍용매장 입고상품"]);
	}
	
	public void getsInfo() {
		System.out.print("번호\t상품명\t가격\t색상\n");
		System.out.println("===========================");
		System.out.println(num+"\t"+sName+"\t"+sPrice+"\t"+sColor);
	}
}

public class ArrReview_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
