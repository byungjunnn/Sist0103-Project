package day0112;

class Food{
	
	private String fName;
	private int fPrice;
	
	static int no;
	
	//명시적 생성자(2개)
	public Food(String fName, int fPrice)) {
		this.fName=fName;
		this.fPrice=fPrice;
	}
	
	//출력문
	public static void fView() {
		no++;
		System.out.println("[메뉴_"+no+"]");
		System.out.println("음식명: "+fName);
		System.out.println("가격: "+fPrice);
	} 
}
