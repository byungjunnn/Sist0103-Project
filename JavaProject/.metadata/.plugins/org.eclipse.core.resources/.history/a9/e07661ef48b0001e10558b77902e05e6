package day0111;

class MyInfo{
	private String name;
	private String mbti;
	private int age;
	
	public MyInfo(String name, String mbti, int age) {
		this.name=name;
		this.mbti=mbti;
		this.age=age;
	}
	
	public void title() {
		System.out.println(name+"\t"+mbti+"\t"+age);
		
	}
}
