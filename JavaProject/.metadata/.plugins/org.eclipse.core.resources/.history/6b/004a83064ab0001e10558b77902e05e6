package day0111;

class MyInfo{
	private String name;
	private String mbti;
	private int age;
	
	public MyInfo(String name, String mbti, int age) {
		this.name=name;
		this.mbti=mbti;
		this.age=age;
	}
	
	public void MyInfoTitle() {
		System.out.println("이름\tMBTI\t나이");
	}
	
	public void MyInfoShow() {
		System.out.println(name+"\t"+mbti+"\t"+age+"세");
	}
}
public class QuizArrObTest_12 {

	public static void main(String[] args) {
		
		MyInfo[] info= {
				new MyInfo("이효리", "ENTP", 26),
				new MyInfo("이상순", "", 0),
				new MyInfo("이영자", "", 0)
		}
		for(int i;i<info.length;i++) {
			info[i].MyInfoShow();
		}
	}

}
