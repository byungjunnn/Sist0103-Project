package day0112;
//부모클래스
class Car{
	
	private String carCompany;
	private String carKind;
	
	public Car() {
	}this(carCompany, carKind)
	
	public Car(String carCompany, String carKind) {
		this.carCompany=carCompany;
		this.carKind=carKind;
	}
}
