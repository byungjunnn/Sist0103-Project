package day0111;

class MyInfo{
	private String name;
	private String mbti;
	private int age;
	
	public MyInfo(String name, String mbti, int age) {
		this.name=name;
		this.mbti=mbti;
		this.age=age;
	}
	
	public void MyInfoTitle() {
		System.out.println("이름\tMBTI\t나이);
	}
	
	public void MyInfoShow() {
		System.out.println();
	}
}
